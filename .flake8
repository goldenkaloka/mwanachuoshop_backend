[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    */migrations/*,
    */staticfiles/*,
    */media/*,
    */venv/*,
    */env/*,
    build,
    dist,
    *.egg-info
per-file-ignores =
    __init__.py:F401
    */migrations/*:E501
    */settings/*.py:E501 